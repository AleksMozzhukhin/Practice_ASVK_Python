git
git init --bare repo      #создание репозитория удалённого
git clone repo prac     #попытка склонировать удалённого репозитория в созданный локальный
#все файлы, начинающиеся с точки, считаются скрытыми
git status #показывает текущее состояние гита
git add #добавляет в будущий комит перечисленные файлы
git commit #делает комит 
